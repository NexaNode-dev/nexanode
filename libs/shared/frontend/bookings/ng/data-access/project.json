{
  "name": "frontend-bookings-ng-data-access",
  "$schema": "../../../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/frontend/bookings/ng/data-access/src",
  "prefix": "lib",
  "projectType": "library",
  "tags": [
    "scope:frontend",
    "type:data-access",
    "tech:angular",
    "domain:bookings"
  ],
  "targets": {
    "build": {
      "executor": "@nx/angular:ng-packagr-lite",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/shared/frontend/bookings/ng/data-access/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/shared/frontend/bookings/ng/data-access/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/shared/frontend/bookings/ng/data-access/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/shared/frontend/bookings/ng/data-access/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
