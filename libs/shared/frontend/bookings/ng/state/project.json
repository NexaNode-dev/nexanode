{
  "name": "frontend-bookings-ng-state",
  "$schema": "../../../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/frontend/bookings/ng/state/src",
  "prefix": "lib",
  "projectType": "library",
  "tags": ["scope:frontend", "type:state", "tech:angular", "domain:bookings"],
  "targets": {
    "build": {
      "executor": "@nx/angular:ng-packagr-lite",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/shared/frontend/bookings/ng/state/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/shared/frontend/bookings/ng/state/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/shared/frontend/bookings/ng/state/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/shared/frontend/bookings/ng/state/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
