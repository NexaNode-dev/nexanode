= 'cover-header'\n          ? this.renderImage(config)\n          : null}\n        {this.header ? <div class=\"card-header\">{this.header}</div> : null}\n        {config.imagePosition === 'top-left' ||\n        config.imagePosition === 'top-right'\n          ? this.renderImage(config)\n          : null}\n        <div class=\"card-content\">\n          {config.imagePosition === 'inside-content'\n            ? this.renderImage(config)\n            : null}\n          <p innerHTML={this.content}></p>\n        </div>\n        {this.footer ? <div class=\"card-footer\">{this.footer}</div> : null}\n      </div>\n    );\n  }\n}\n"],"mappings":"2CAAA,MAAMA,EAAkB,yQACxB,MAAAC,EAAeD,E,MCYFE,EAAY,M,kHAMf,gBAAAC,GACN,OAAOC,KAAKC,MAAMC,KAAKC,QAAU,K,CAGnC,WAAAC,CAAYD,GACV,IAAKA,EAAOE,SAAU,OAAO,KAE7B,MAAMC,EAAU,cAAcH,EAAOI,eAAiB,KACtD,OACEC,EAAA,OACEC,IAAKN,EAAOE,SACZK,IAAKP,EAAOQ,UAAY,aACxBC,MAAON,G,CAKb,MAAAO,GACE,MAAMV,EAASD,KAAKH,mBAEpB,OACES,EAAA,OAAAM,IAAA,2CAAKF,MAAM,QACRT,EAAOI,gBAAkB,eACtBL,KAAKE,YAAYD,GACjB,KACHD,KAAKa,OAASP,EAAA,OAAKI,MAAM,eAAeV,KAAKa,QAAgB,KAC7DZ,EAAOI,gBAAkB,YAC1BJ,EAAOI,gBAAkB,YACrBL,KAAKE,YAAYD,GACjB,KACJK,EAAA,OAAAM,IAAA,2CAAKF,MAAM,gBACRT,EAAOI,gBAAkB,iBACtBL,KAAKE,YAAYD,GACjB,KACJK,EAAA,KAAAM,IAAA,2CAAGE,UAAWd,KAAKe,WAEpBf,KAAKgB,OAASV,EAAA,OAAKI,MAAM,eAAeV,KAAKgB,QAAgB,K"}