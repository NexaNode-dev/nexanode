{"version":3,"names":["nexanodeCardCss","NexanodeCardStyle0","NexanodeCard","getDefaultConfig","JSON","parse","this","config","renderImage","imageSrc","classes","imagePosition","h","src","alt","imageAlt","class","render","key","header","innerHTML","content","footer"],"sources":["../../../../../../libs/shared/frontend/layout/util/src/components/nexanode-card/nexanode-card.css?tag=nexanode-card&encapsulation=shadow","../../../../../../libs/shared/frontend/layout/util/src/components/nexanode-card/nexanode-card.tsx"],"sourcesContent":[":host {\n  display: block;\n}\n\n.card {\n  display: block;\n  background-color: var(--primary-light, #fff);\n  border-radius: 8px;\n  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\n  overflow: hidden;\n  transition: box-shadow 0.3s ease-in-out;\n  width: 45vw; /* Adjust based on your layout */\n  margin: 16px;\n  height: auto;\n}\n\n.card:hover {\n  box-shadow: 0 8px 12px rgba(0, 0, 0, 0.2);\n}\n\n.card-header,\n.card-footer {\n  background-color: var(--secondary-light, #f0f0f0);\n  padding: 12px 20px;\n}\n\n.card-header {\n  display: flex;\n  align-items: center;\n  padding: 1rem;\n  font-size: 2rem;\n}\n\n.card-title {\n  font-size: 20px;\n  margin: 0;\n}\n\n.card-content {\n  padding: 20px;\n}\n\n.card-image {\n  display: block;\n  width: 5rem;\n  height: auto;\n  aspect-ratio: 1/1;\n  object-fit: cover;\n}\n\n/* Position-specific styles */\n.card-image.top-left {\n  width: 50px; /* Smaller size for corner placement */\n  position: absolute;\n  top: 8px;\n  left: 8px;\n}\n\n.card-image.top-right {\n  width: 150px; /* Adjust as necessary */\n  position: absolute;\n  top: 8px;\n  right: 8px;\n}\n\n.card-image.cover-header {\n  width: 100%;\n  height: 180px; /* Adjust height for your needs */\n  object-fit: cover; /* Ensure the image covers the area */\n}\n\n.justify-between {\n  justify-content: space-between;\n}\n\n.flex-row-reverse {\n  flex-direction: row-reverse;\n}\n\n.header-content {\n  flex-grow: 1;\n}\n\n/* Additional responsive adjustments */\n@media (max-width: 768px) {\n  .card {\n    max-width: 100%;\n  }\n}\n","import { Component, h, Prop } from '@stencil/core';\ninterface CardConfig {\n  imageSrc?: string;\n  imageAlt?: string;\n  imagePosition?: 'top-left' | 'top-right' | 'cover-header' | 'inside-content';\n  // Add other configuration options as needed\n}\n\n@Component({\n  tag: 'nexanode-card',\n  styleUrl: 'nexanode-card.css',\n  shadow: true,\n})\nexport class NexanodeCard {\n  @Prop() header: string;\n  @Prop() content: string;\n  @Prop() footer: string;\n  @Prop() config: string;\n\n  private getDefaultConfig(): CardConfig {\n    return JSON.parse(this.config || '{}');\n  }\n\n  renderImage(config: CardConfig) {\n    if (!config.imageSrc) return null;\n\n    const classes = { 'card-header': true, 'justify-between': config.imagePosition === 'top-right', 'flex-row-reverse': config.imagePosition === 'top-right' };\n    return (\n      <img\n        src={config.imageSrc}\n        alt={config.imageAlt || 'Card Image'}\n        class={classes}\n      />\n    );\n  }\n\n  render() {\n    const config = this.getDefaultConfig();\n\n    return (\n      <div class=\"card\">\n        {config.imagePosition === 'cover-header'\n          ? this.renderImage(config)\n          : null}\n        {this.header ? (\n          <div class=\"card-header\">\n            {config.imagePosition === 'top-left' ||\n            config.imagePosition === 'top-right'\n              ? this.renderImage(config)\n              : null}\n            <div class=\"header-content\">\n              <h2 class=\"card-title\">{this.header}</h2>\n            </div>\n          </div>\n        ) : null}\n\n        <div class=\"card-content\">\n          {config.imagePosition === 'inside-content'\n            ? this.renderImage(config)\n            : null}\n          <p innerHTML={this.content}></p>\n        </div>\n        {this.footer ? <div class=\"card-footer\">{this.footer}</div> : null}\n      </div>\n    );\n  }\n}\n"],"map